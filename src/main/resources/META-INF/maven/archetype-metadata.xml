<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.2.0 https://maven.apache.org/xsd/archetype-descriptor-1.2.0.xsd" name="langp-plugin-archetype"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="Name">
      <defaultValue>Demo</defaultValue>
    </requiredProperty>
    <requiredProperty key="Description">
      <defaultValue>Demo</defaultValue>
    </requiredProperty>
    <requiredProperty key="Version">
      <defaultValue>1.0.0</defaultValue>
    </requiredProperty>
    <requiredProperty key="Type">
      <defaultValue>tool</defaultValue>
    </requiredProperty>
    <requiredProperty key="Author">
      <defaultValue>langp.cn</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
        <include>**/.gitkeep</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/resources</directory>
      <includes>
        <include>**/*.yaml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <includes>
        <include>.gitignore</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>